#include <stdio.h>
#include <stdarg.h>

int sumnum(int count, ...) {
	va_list args;
	va_start(args, count); /*initialize with count as last parameter*/
	int sum = 0,i;
	for (i = 0; i < count; i++) {
		sum += va_arg(args, int); /* retrive next argument as int*/
	}
	va_end(args);
	return sum;
}

int main(int argc, char** argv) {
	printf("Sum: %d\n", sumnum(3,2,5,7));
	printf("Sum: %d\n", sumnum(2,10,20));
	return 0;
}
